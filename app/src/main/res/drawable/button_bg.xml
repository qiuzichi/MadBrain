<?xml version="1.0" encoding="utf-8"?>
<!-- 取消按钮效果 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 选中未up时 -->
    <item android:state_pressed="true" ><shape>
            <solid android:color="@color/button_bg_press_gray" />

            <corners android:radius="7dp" />

            <!-- 按钮文字和边缘距离(内边距) -->

        </shape></item>

    <!-- 焦点 -->
    <item ><shape>
        <solid android:color="@color/button_bg_press_blue" />

        <corners android:radius="7dp" />

        <!-- 按钮文字和边缘距离(内边距) -->

    </shape></item>

</selector>