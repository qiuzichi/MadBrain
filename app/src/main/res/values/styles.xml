<resources>
    <!-- 等待的进度条样式 -->
    <style name="wait_progressbar_rotate">
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateDrawable"> @drawable/wait_progress
        </item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:minWidth">50dip</item>
        <item name="android:maxWidth">50dip</item>
        <item name="android:minHeight">50dip</item>
        <item name="android:maxHeight">50dip</item>
    </style>
    <!--dialog style begin-->
    <style name="dialog_wait" parent="android:style/Theme.Holo.Dialog">
        <item name="android:windowBackground"> @drawable/y_tips_bg
        </item>
    </style>

    <style name="dlg_title_white_20dip_wwrap_hwrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">22dip</item>
    </style>
    <style name="dialog" parent="android:style/Theme.Holo.DialogWhenLarge">
        <item name="android:windowBackground">@drawable/dialog_background</item>
    </style>
    <style name="dlg_body_666666_24dip_wwrap_hwrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">24dip</item>
    </style>
    <!--dialog style end-->
    <style name="AppTheme" parent="@android:style/Theme.Black.NoTitleBar.Fullscreen">
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="MainLfIBtn">
        <item name="android:layout_width">@dimen/main_lf_image_wh</item>
        <item name="android:layout_height">@dimen/main_lf_image_wh</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginLeft">@dimen/main_lf_padding</item>
        <item name="android:layout_marginRight">@dimen/main_lf_padding</item>
    </style>

    <style name="TextMarquee">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>

    <style name="locatingProgressBar">
        <item name="android:indeterminateDuration">1000</item>
        <item name="android:layout_width">17dip</item>
        <item name="android:layout_height">17dip</item>
        <item name="android:indeterminateDrawable">@drawable/locating_rotate_anim</item>
    </style>

    <style name="KeyboardBtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">3dp</item>
    </style>

    <style name="ListenNumberBtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>
    <style name="DialogStyle">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

    <style name="EditText">
        <item name="android:ems">20</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:maxLength">20</item>
        <item name="android:paddingLeft">6dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/txt_ash</item>
        <item name="android:background">@drawable/edit_bg</item>
    </style>

    <style name="LoginBottomBtn">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="PersonalItemText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">56dp</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">personalItemClick</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/common_text_22</item>
        <item name="android:drawablePadding">6dip</item>
    </style>

    <!-- 修改密码公用属性 -->
    <style name="forgest_editext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@null</item>
        <item name="android:ems">20</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:maxLength">20</item>
        <item name="android:paddingLeft">5dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/txt_ash</item>
    </style>

    <style name="rl_properties">
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <!--<item name="android:background">@drawable/yj_add</item>-->
    </style>


    <style name="animation">
        <item name="android:windowEnterAnimation">@anim/translats_status_in</item>
        <item name="android:windowExitAnimation">@anim/translats_status_out</item>
    </style>

    <style name="DialogTheme" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@drawable/bg_transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>


    <style name="mystyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/head_in</item>
         //进入时的动画;
        <item name="@android:windowExitAnimation">@anim/head_out</item>
        //退出时的动画
    </style>

    <style name="ItemSmallBlackText" parent="TextSmall">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextSmall">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="PersonalFrgInfoText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/radius_4dip</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/personal_authentication_text_step</item>
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:drawablePadding">@dimen/radius_4dip</item>
    </style>

    <style name="PersonalFrgInfoEdit">
        <item name="android:layout_width">230dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:background">@drawable/personal_edit_bg</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">#515151</item>
        <item name="android:textColorHint">@color/edit_text_hint</item>
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:padding">@dimen/radius_4dip</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="PersonalFrgInfoLinearLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_margin">8dp</item>
    </style>

    <style name="PersonalAuthenticationStepText" parent="PersonalFrgInfoText">
        <item name="android:layout_marginLeft">@dimen/radius_4dip</item>
        <item name="android:textSize">@dimen/person_text_age_s</item>
    </style>

    <style name="PersonalAuthenticationTextType" parent="PersonalFrgInfoText">
        <item name="android:layout_marginRight">@dimen/main_lf_image_wh</item>
        <item name="android:textSize">@dimen/person_text_age_s</item>
        <item name="android:textColor">@color/personal_authentication_text_step</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="PersonalFrgSettingText" parent="PersonalFrgInfoText">
        <item name="android:layout_marginRight">@dimen/main_lf_image_wh</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">@color/personal_setting_text</item>
    </style>

    <style name="SettingPwdEditText" parent="EditText">
        <item name="android:layout_height">@dimen/set_login_pwd_pic_h</item>
        <item name="android:layout_width">210dp</item>
        <item name="android:textSize">19sp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textColor">@color/personal_setting_text</item>
        <item name="android:textColorHint">@color/edit_text_hint</item>
    </style>


    <style name="SettingPwdPic">
        <item name="android:layout_height">@dimen/set_login_pwd_pic_h</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginRight">-6dp</item>
        <item name="android:src">@drawable/user_pwd_image</item>
    </style>

    <style name="PersonalFrgRecordText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">@dimen/radius_4dip</item>
    </style>
    <style name="PersonalFrgRecordBtn">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">@dimen/setting_text_padding_30</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_size_18</item>
    </style>

    <style name="PersonalFrgRecordEdit">
        <item name="android:layout_width">114dp</item>
        <item name="android:layout_height">@dimen/setting_text_padding_30</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/binary_line_layout_padding</item>
        <item name="android:paddingRight">@dimen/binary_line_layout_padding</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/text_size_18</item>
        <item name="android:background">@drawable/personal_record_edit_bg</item>
        <item name="android:textColorHint">@color/edit_text_hint</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="Spinner">
        <item name="android:ems">20</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:maxLength">20</item>
        <item name="android:paddingLeft">6dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/txt_ash</item>
        <item name="android:background">@drawable/edit_bg</item>
    </style>

    <style name="colleague_custom_animation">
        <item name="android:windowEnterAnimation">@anim/colleague_right_in</item>
        <item name="android:windowExitAnimation">@anim/colleague_right_out</item>
    </style>

    <style name="NotificationContent">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>
    -

    <style name="NotificationTitle">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>
    -

    <style name="btn_custom_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center|right</item>
        <item name="android:background">?android:listChoiceBackgroundIndicator</item>
    </style>


</resources>
