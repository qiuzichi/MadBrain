<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/group_historry_list"
    android:layout_width="fill_parent"
    android:layout_height="50dp"
    android:gravity="left|center_vertical"
    android:layout_marginTop="50dp"
    android:orientation="horizontal">
    <TextView
        android:id="@+id/matchId"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="180dp"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"/>
    <TextView
        android:id="@+id/projectId"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"/>
    <TextView
        android:id="@+id/startDate"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="@dimen/hisrecord_widht_answer"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"/>
    <TextView
        android:id="@+id/rectime"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"/>
    <TextView
        android:id="@+id/memtime"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/score"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="@dimen/hisrecord_widht_answer_mode"
        android:layout_height="match_parent"
        android:background="@drawable/shapee"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/ranking"
        android:textSize="@dimen/setting_text_18sp"
        android:layout_width="@dimen/hisrecord_widht_answer_mode"
        android:background="@drawable/shapee"
        android:layout_height="match_parent"
        android:gravity="center"
        />
</TableRow>

